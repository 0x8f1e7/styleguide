.theme-dark
  background: $bg-color-dark;
  color: $color-text-contrast

  h1, h2, h3, h4, h5
    color: $color-text-contrast

  blockquote
    color $color-text-contrast
    author
      color $color-text-contrast

.theme-light
  color: $color-text
  h1,h2,h3,h4,h5,blockquote
    color: $color-text
