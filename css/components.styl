/*==========  Footer  ==========*/
footer.site-footer
  margin-top: 10px;
  margin-bottom: 10px;
  font-size: 12px;
  padding: 20px 0;
  color: #999;
  nav 
    text-align: right
    float: right
    li 
      display: inline-block
      margin-left: 30px;
  a 
    color: #999;

  .col-md-11
    float: right

@media screen and (max-width: 768px)
  footer.site-footer
    .col-md-11
      float: none;
    nav 
      float: none;
      display: block
      clear:both;
      text-align: left;
      margin: 0;
      padding: 0;
      margin-bottom: 14px;
      ul 
        margin: 0;
        border-bottom: 1px solid #f1f1f1;

        padding: 0;
      li
        display: block
        margin: 0;
      a 
        display: block
        border-top: 1px solid #f1f1f1;
        margin: 0;
        padding: 14px 0;



/*==========  Banner  ==========*/

.banner
  color: white;
  text-align: center
  padding-top: 60px;
  padding-bottom: 60px;
  margin-bottom: 50px;
  position: relative;
  overflow: hidden
  background-color: #14204d;
  background: rgb(20,32,77); /* Old browsers */
  background: -moz-linear-gradient(120deg,  rgba(20,32,77,1) 0%, rgba(60,200,244,1) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(20,32,77,1)), color-stop(100%,rgba(60,200,244,1))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(120deg,  rgba(20,32,77,1) 0%,rgba(60,200,244,1) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(120deg,  rgba(20,32,77,1) 0%,rgba(60,200,244,1) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(120deg,  rgba(20,32,77,1) 0%,rgba(60,200,244,1) 100%); /* IE10+ */
  background: linear-gradient(120deg,  rgba(20,32,77,1) 0%,rgba(60,200,244,1) 100%); /* W3C */

  h1
    font-size: 270%;
    // margin-top: 0;
    color: white;
  p
    font-size: 120%;
    max-width: 580px;
    margin: auto;
    margin-top: 20px;
    line-height: 170%;
    color: rgba(255,255,255,.8);


  i
    font-size: 50px;
    -webkit-animation: icon_banner 1s;
    position: relative;

  .btn
    margin-top: 10px;

  em
    display: block
    font-size: 12px;
    margin-top: 4px;
    letter-spacing: 1px;


@keyframes icon_banner
  from
    top: -100px;
    opacity: 0;
  to
    opacity: 1;
    top: 0;


@media screen and (max-width: 660px)
  .banner
    padding: 20px 0;
    h1
      font-size: 36px


