// Fakt

// Blond - font-weight: 300

@font-face {
  font-family: 'Fakt-Web';
  src: url("//cdn.auth0.com/styleguide/core/development/fonts/fakt/FaktPro-Blond.eot?#iefix");
  src: url("//cdn.auth0.com/styleguide/core/development/fonts/fakt/FaktPro-Blond.eot?#iefix") format("eot"),
       url("//cdn.auth0.com/styleguide/core/development/fonts/fakt/FaktPro-Blond.woff") format("woff"),
       url("//cdn.auth0.com/styleguide/core/development/fonts/fakt/FaktPro-Blond.ttf") format("truetype")
  font-weight: 300;
  font-style: normal;
}

@font-face {
